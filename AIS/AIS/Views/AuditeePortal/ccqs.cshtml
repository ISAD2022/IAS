@{
    ViewData["Title"] = "CCQs";
    Layout = "_Layout";
}
<h3 style=" display:block;color: #45c545;" class="mt-3">Compliance & Control Questionnaire</h3>

<div class="row col-md-12 mt-3">
    <table class="table table-hover table-bordered table-hover mt-3 table-striped">
        <thead style="background-color: rgb(181 229 117 / 93%) !important; ">
            <tr>
                 <th class="col-md-auto">SR No</th>
                <th class="col-md-auto">Audit Entity</th>
                <!-- <th class="col-md-2">Reference</th>-->
                <th class="col-md-auto">Questions</th>
                <th class="col-md-auto">Control Violation</th>
                <th class="col-md-auto">Risk</th>
                <th class="col-md-auto">Status</th>

            </tr>
        </thead>
        
        <tbody>

            @{
               

                    foreach (var item in (dynamic)(ViewData["AUDITCCQ"]))
                    {
                        <tr>
                            <td align="justify">@item.ID</td>
                            <td align="left">@item.ENTITY_ID</td>
                            <td align="justify">@item.QUESTIONS</td>
                            <td align="right">@item.CONTROL_VIOLATION_ID</td>
                            <td align="right">@item.RISK_ID</td>
                            <td align="right">@item.STATUS</td>

                        </tr>

                    }

                //}
            }

        </tbody>
    </table>
</div>
