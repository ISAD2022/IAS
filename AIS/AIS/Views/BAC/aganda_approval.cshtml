@{
    ViewData["Title"] = "BAC Agenda Approval";
    Layout = "_Layout";
}

<script>

    $(document).ready(function () {

        $('#bacresponse').richText({
            imageUpload: false,
            fileUpload: false,
            videoEmbed: false,
            urls: false

        });
    });

    function bacassignmentresponse() {


        $('#bacassignmentresponse').modal('show');

    }


</script>
<div class="row col-md-12 mt-3">

    <h3 style="color: #45c545;">BAC Agenda Approval</h3>
</div>
<div class="row col-md-12 mt-4">

    <label class="font-weight-bold">BAC Meeting Number</label>


    <input id="bacresponseattachment" style="max-height:150px; height:auto;" class="form-control" />

</div>
<div class="row col-md-12 mt-3">
    <table id="manageObsPanel" class="table table-hover table-bordered table-hover mt-3 table-striped">
        <thead style="background-color: rgb(181 229 117 / 93%) !important; ">
            <tr>
                <th class="col-md-1">Sr No.</th>
                <th class="col-md-2">Memo No.</th>
                <th class="col-md-3">Subject</th>
                <th class="col-md-5">Remarks</th>

            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>
<div class="row col-md-12 mt-3 modal-footer">
    <button onclick="publishNewAuditPlanChanges();" type="button" class="btn btn-danger">Reffered Back</button>
    <button type="button" class="btn btn-primary" data-dismiss="modal">Aproved</button>
</div>
