@{
    ViewData["Title"] = "Home";
    Layout = "_Layout";
}
<style type="text/css">
    .container{
        max-width:100%;
    }
    .card-header {
        background-color: transparent !important;
        font-size: 20px;
        height: 100px;
    }
    .icon-card{
        margin:10px;
    }
</style>

<script type="text/javascript">
    $(document).ready(function () {
        var doc = new jsPDF();
        var specialElementHandlers = {
            '#editor': function (element, renderer) {
                return true;
            }
        };

        $('#cmd').click(function () {
            doc.fromHTML($('#content').html(), 15, 15, {
                'width': 170,
                'elementHandlers': specialElementHandlers
            });
            doc.save('sample-file.pdf');
        });
    });
</script>


<div class="row col-md-12">
    <div class="col-md-6 mt-3 p-10">
            <img class="w-100" src="~/Images/home.png">
        </div>
    <div class="col-md-6 mt-3 p-10">
        <h4>Notification panel</h4>
        <table class="table table-bordered mb-0 mt-3 bg-white table-striped">
            <thead>

                <tr>
                    <th class="col-md-2">From</th>
                    <th class="col-md-2">Type</th>
                    <th class="col-md-2">Subject</th>
                    <th class="col-md-2">Sent Date</th>
                    <th class="col-md-2">Due Date</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td colspan="5">Notifications are ready and will be plug in after getting officail Emails once the System will move in Production</td>
                   
                </tr>
               

            </tbody>

        </table>
        <div class="col-md-12">
           
            <button id="cmd" class="btn btn-primary">Export PDF</button>
        </div>
    </div>
    
</div>
