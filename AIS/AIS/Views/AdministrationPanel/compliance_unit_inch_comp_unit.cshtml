</script>
<div class="row mt-3">
    <div class="row col-md-12">
        <div class="col-md-6" style="padding-left:0px;">
            <h2 style="color: #45c545;">Compliance Unit</h2>
        </div>
    </div>
</div>
<div id="listOfAuditPeriodsContainer" class="row mt-3">

    <br />
    <table id="listOfAuditPeriods" class="table table-hover table-bordered table-hover mt-3 table-striped">
        <thead style="background-color: rgb(181 229 117 / 93%) !important;">
            <tr>
                <th class="col-md-1">Sr No.</th>
                <th class="col-md-4">Compliance Unit</th>
                <th class="col-md-2">Incharge Unit</th>
                <th class="col-md-2">Settelement Officer</th>
                <th class="col-md-1">Status</th>
                <th class="col-md-1"></th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>




<div id="complianceUnit" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-dialog-scrollable modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header bg-danger text-white">
                <h5 class="modal-title">Compliance Unit</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row col-md-6">
                 <h6>Incharge Compliance Unit / Authorizer</h6>
                </div>

                <div class="row col-md-12 mt-1">
                    <div class="col-md-2">
                        <label>P.P.No.</label>
                    </div>
                    <div class="col-md-4">
                        <input id="ppnoSearchField" type="text" class="form-control" placeholder="P.P.Number" aria-label="P.P.Number" aria-describedby="basic-addon2">
                    </div>
                    <div class="col-md-2">
                      
                        <button id="searchbtn" class="btn btn-danger col-md-2" style="margin-left:auto;">Search</button>
                    </div>
                </div>


                <div class="row col-md-6">
                    <h6>Settelement Officer </h6>
                </div>

                <div class="row col-md-12 mt-1">
                    <div class="col-md-2">
                        <label>P.P.No.</label>
                    </div>
                    <div class="col-md-4">
                        <input id="ppnoSearch" type="text" class="form-control" placeholder="P.P.Number" aria-label="P.P.Number" aria-describedby="basic-addon2">
                    </div>
                    <div class="col-md-2">
                     
                        <button id="searchbtn" class="btn btn-danger col-md-2" style="margin-left:auto;">Search</button>
                    </div>
                </div>

            </div>

            <div class="modal-footer">
              
                <button type="button" class="btn btn-danger">Save</button>
            </div>
        </div>
    </div>
</div>

