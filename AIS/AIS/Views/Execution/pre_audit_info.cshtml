@{
    ViewData["Title"] = "Pre Audit Information";
    Layout = "_Layout";
}


<div class="row col-md-12 mt-3">
    <h4 style="display: block; color: #45c545;">Pre Audit Information</h4>
</div>

<div class="row col-md-12 btn-group">
    <div class="row col-md-12">
        <button type="button" class="col-md-4 btn mt-3 btn-success" onclick="window.location.href = '/reports/glhead_summary';">GL Head Details</button>
    </div>
    <div class="row col-md-12">
        <button type="button" class="col-md-4 btn mt-3 btn-success" onclick="window.location.href = '/Execution/loan_case_details';">Loan Case</button>
    </div>
    <div class="row col-md-12">
        <button type="button" class="col-md-4 mt-3 btn btn-success" onclick="window.location.href = '/Reports/income_expenditure';">Income/Expenditure</button>
    </div>
    <div class="row col-md-12">
        <button type="button" class="col-md-4 mt-3 btn btn-success" onclick="window.location.href = '/Execution/branch_deposit_info';">Deposit</button>
        
    </div>
    <div class="row col-md-12">
        <button type="button" class="col-md-4 mt-3 btn btn-success" onclick="window.location.href = '/Execution/disb_info';">Disbursement</button>
    </div>
</div>
