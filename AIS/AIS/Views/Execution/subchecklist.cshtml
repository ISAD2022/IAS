@{
    ViewData["Title"] = "Sub Checklist";
    Layout = "_Layout";
}
<script type="text/javascript">
    var g_endId = 0;
    $(document).ready(function () {
        var url_string = window.location;
        var url = new URL(url_string);
        var checklist_id = url.searchParams.get("id");
        g_endId = url.searchParams.get("engId");
        $.ajax({
            url: "/ApiCalls/sub_checklist",
            type: "POST",
            data: {
                'T_ID': checklist_id,
                'ENG_ID': g_endId
            },
            cache: false,
            success: function (data) {
                console.log('subhcekclist', data);
                $('#subchecklistPanel tbody').empty();
                var sr = 1;
               $.each(data, function (i, v) {
                   $('#subchecklistPanel tbody').append('<tr id="' + v.s_ID + '"><td>' + sr + '</td><td>' + v.t_NAME + '</td><td>' + v.heading + '</td><td class="text-center"><a href="#" onclick="proceedToChecklistDetails(' + v.s_ID+')">Checklist Details</a></td><td class="text-center"><span class="badge badge-danger rounded-pill d-inline">' + v.status + '</span></td></tr>');
                   sr++;
                });

            },
            dataType: "json",
        });
    });
    function proceedToChecklistDetails(id) {
     window.location.href = '/Execution/checklist_details?engId=' + g_endId + '&id=' + id;
    }
</script>
<div class="row mt-3">
    <div>
        <center>
            <h3 style="display: block; color: #45c545;">Sub List</h3>
        </center>
    </div>

    <table id="subchecklistPanel" class="table table-bordered mb-0 mt-3 bg-white table-striped">
        <thead>
            <tr>
                <th class="col-md-1 font-weight-bold">Sr. No</th>
                <th class="col-md-2 font-weight-bold">Parent/Process</th>
                <th class="col-md-4 font-weight-bold">Heading/Sub List</th>
                <th class="col-md-2 font-weight-bold"></th>
                <th class="col-md-1 font-weight-bold text-center">Status</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>
