@{
    ViewData["Title"] = "FAD_DSA_Position_Fortnightly";
    Layout = "_Layout";
}
<script>
    $(document).ready(function () {
        $("#searchTableRecord").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#A_P_E_T tbody tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });


    });
</script>

<div class="row col-md-12 mt-3">
    <h3 style=" display:block;color: #45c545;">FAD DSA Position Fortnightly</h3>
</div>

<div class="row col-md-12 mt-2">
    <div style=" width:155px">
        <label for="periodStartModalField"><b>Operational Zone</b><span class="text-danger">*</span></label>
    </div>
    <div class=" col-md-5 mr-1">
        <select id="entitySelectField" onchange="" class="form-control form-select">
            <option value="0" id="0">--Select Operational Zone--</option>
        </select>
    </div>

    <div style=" width:85px">
        <label for="periodEndModalField"><b>Sent To</b><span class="text-danger">*</span></label>
    </div>
    <div style=" width:595px">
        <select id="entitySelectField" onchange="" class="form-control form-select">
            <option value="0" id="0">--Select--</option>
        </select>
    </div>
</div>



<div class="row col-md-12 mt-1">
    <button id="findButton" onclick="" class="btn btn-danger" style="margin-left:auto;">Search</button>

</div>



<div class="row col-md-12 mt-3 mb-2">

    <input class="form-control" id="searchTableRecord" type="text" placeholder="Search Results..">

</div>

<div class="row col-md-12 mt-3">
    <table id="zoneiwseperformance" class="table table-hover table-bordered table-hover mt-3 table-striped" style="font-size:small; text-align:center">
        <thead style="background-color: rgb(181 229 117 / 93%) !important; ">
            <tr>
                <th class="col-md-auto" rowspan="2">Sr. No</th>

                <th class="col-md-auto" rowspan="2">Operational Zone</th>
                <th class="col-md-auto" rowspan="2">Total DSAs sent by AZ</th>
                <th class="col-md-auto" colspan="7">Status of DSAs at FAD Level</th>


            </tr>

            <tr style="text-align:center">
                <td class="col-md-auto">Sent to DPD</td>
                <td class="col-md-auto">Sent to BOD</td>

                <td class="col-md-auto">Sent to CAD</td>
                <td class="col-md-auto">Sent to MD KSSL/HR</td>
                <td class="col-md-auto">Referred back</td>
                <td class="col-md-auto">Not received</td>
                <td class="col-md-auto">Pending</td>

            </tr>
        </thead>
        <tbody>
            @{
                if (ViewData["ZONEWISEPERFORMANCE"] != null)
                {

                    var index = 1;
                    foreach (var item in (dynamic)(ViewData["ZONEWISEPERFORMANCE"]))
                    {

                        <tr>
                            <td>@index</td>
                            <td>@item.ZONENAME</td>
                            <td>@item.PARA_TOTAL</td>
                            <td>@item.PARA_PENDING</td>
                            <td>@item.PARA_ENTERED</td>
                        </tr>
                        index++;
                    }
                }
            }
        </tbody>

    </table>
</div>

<div class="col-md-12 offset-md-12 mt-4">
    <button id="printPreview_btn" class="btn btn-danger" style="float:right; margin-bottom:10px;">Download Report/Print</button>
</div>