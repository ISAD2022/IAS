@{
    ViewData["Title"] = "FAD_Branch_Risk_Rating";
    Layout = "_Layout";
}
<script>
    $(document).ready(function () {
        $("#searchTableRecord").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#A_P_E_T tbody tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });


    });
</script>
<div>
    <div class="row col-md-12 mt-3">
        <h3 style=" display:block;color: #45c545;">FAD Branch Risk Rating</h3>
    </div>
    <div class="row col-md-12 mt-3 mb-2">

        <input class="form-control" id="searchTableRecord" type="text" placeholder="Search Results..">

    </div>

    <div class="row col-md-12 mt-3">
        <table id="zoneiwseperformance" class="table table-hover table-bordered table-hover mt-3 table-striped">
            <thead style="background-color: rgb(181 229 117 / 93%) !important; ">
                <tr>
                    <th class="col-md-auto">Sr. No</th>
                    <th class="col-md-auto">Code</th>
                    <th class="col-md-auto">Audit Zone</th>

                    <th class="col-md-auto">Operational Zone</th>

                    <th class="col-md-auto">Audit period (Audited Till)</th>
                    <th class="col-md-auto">Risk Rating</th>
                    <th class="col-md-auto">Risk Rating Mark</th>
                    <th class="col-md-auto">Rating</th>
                    <th class="col-md-auto">Size</th>
                  



                </tr>
            </thead>
            <tbody>
       
            </tbody>

        </table>
    </div>

</div>
