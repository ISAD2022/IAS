@{
    ViewData["Title"] = "FAD_New_Para_Performance";
    Layout = "_Layout";
}
<script>
    $(document).ready(function () {
        $("#searchTableRecord").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#A_P_E_T tbody tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });


    });
</script>

<div class="row col-md-12 mt-3">
    <h3 style=" display:block;color: #45c545;">FAD New Para Performance</h3>
</div>

<div class="row col-md-12 mt-3 mb-2">

    <input class="form-control" id="searchTableRecord" type="text" placeholder="Search Results..">

</div>

<div class="row col-md-12 mt-3">
    <table id="zoneiwseperformance" class="table table-hover table-bordered table-hover mt-3 table-striped">
        <thead style="background-color: rgb(181 229 117 / 93%) !important; ">
            <tr>


                <th class="col-md-auto">SR#</th>
                <th class="col-md-auto">Audit Zone</th>

                <th class="col-md-auto">Total Paras</th>
                <th class="col-md-auto">On Spot Settled</th>
                <th class="col-md-auto">Un-Settled Paras</th>




            </tr>
        </thead>
        <tbody>
            @{
                if (ViewData["NEWPARAPERFORMANCE"] != null)
                {

                    var index = 1;
                    foreach (var item in (dynamic)(ViewData["NEWPARAPERFORMANCE"]))
                    {

                        <tr>
                            <td>@index</td>
                            <td>@item.AUDIT_ZONE</td>
                            <td>@item.Total_Paras</td>
                            <td>@item.Setteled_Para</td>
                            <td>@item.Unsetteled_Para</td>
                        </tr>
                        index++;
                    }
                }
            }
        </tbody>

    </table>
</div>
<div class="col-md-12 offset-md-12 mt-4"  style="display:flex; align-content:flex-end">
        <button id="printPreview_btn" class="btn btn-danger">Download Report/Print</button>    
</div>

