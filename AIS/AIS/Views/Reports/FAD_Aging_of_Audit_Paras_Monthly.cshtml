@{
    ViewData["Title"] = "FAD_Aging_of_Audit_Paras_Monthly";
    Layout = "_Layout";
}
<script>
    $(document).ready(function () {
        $("#searchTableRecord").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#A_P_E_T tbody tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });


    });
</script>
<div>
    <div class="row col-md-12 mt-3">
        <h3 style=" display:block;color: #45c545;">FAD Aging of Audit Paras Monthly</h3>
    </div>
    <div class="row col-md-12 mt-3 mb-2">

        <input class="form-control" id="searchTableRecord" type="text" placeholder="Search Results..">

    </div>

    <div class="row col-md-12 mt-3">
        <table id="zoneiwseperformance" class="table table-hover table-bordered table-hover mt-3 table-striped" style="font-size:small; text-align:center">
            <thead style="background-color: rgb(181 229 117 / 93%) !important; ">
                <tr>
                    <th class="col-md-auto">Sr. No</th>
                    <th class="col-md-auto">Serious/Normal</th>
                    <th class="col-md-auto">Audit Zone</th>
                    <th class="col-md-auto">Operational Zone</th>
                    <th class="col-md-auto">Branch</th>
                    <th class="col-md-auto">Audit Period</th>
                    <th class="col-md-auto">Paras At Begining Of The Month</th>
                    <th class="col-md-auto">Paras Added During The Month</th>
                    <th class="col-md-auto">Total Paras For Settlement</th>
                    <th class="col-md-auto">Paras Settled During The Month</th>
                    <th class="col-md-auto">Percentage Of Settlement</th>
                    <th class="col-md-auto">Balance At The End Of The Month</th>
                    <th class="col-md-auto">Aging</th>

                </tr>
            </thead>
            <tbody>
                @{
                    if (ViewData["ZONEWISEPERFORMANCE"] != null)
                    {

                        var index = 1;
                        foreach (var item in (dynamic)(ViewData["ZONEWISEPERFORMANCE"]))
                        {

                            <tr>
                                <td>@index</td>
                                <td>@item.ZONENAME</td>
                                <td>@item.PARA_TOTAL</td>
                                <td>@item.PARA_PENDING</td>
                                <td>@item.PARA_ENTERED</td>
                            </tr>
                            index++;
                        }
                    }
                }
            </tbody>

        </table>
    </div>

</div>
